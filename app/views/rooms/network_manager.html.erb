<div class="card mb-3 bg-dark text-light">
  <div class="card-header h3">
    <%= icon('fas','igloo', t('.title', name: @room.name), class: 'fa-fw h2 mb-0') %>
  </div>
  <div class="card-body">
    <%= form_with(model: @room, local: true) do |form| %>
      <div class="form-group">
        <div class="form-row">
          <div class="col-md-6">
            <div class="form-group">
              <%= form.label :name %>
              <%= form.text_field :name, disabled: true, class: "form-control" %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

<%= render('computers_manager') %>
